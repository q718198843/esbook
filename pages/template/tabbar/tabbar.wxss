/* pages/template/tabbar/tabbar.wxss */
.tabbar{
  height: 162rpx;
  width: 100%;
  position: fixed;
  left: 0;
  bottom: 0;
  display: flex;
  justify-content: space-around; 
  align-items: center;
  z-index: 98;
  background-size: 100% 100%
}
.icon{
  display: flex;
  justify-content: space-around;
  flex-direction: column;
  align-items: center;
  font-size: 21rpx;
  font-family: PingFang-SC-Light;
  width: 33.33%
}
.bj{
  height: 162rpx;
  width: 100%;
  position: absolute;
  left: 0;
  top: 0;
  z-index: -1
}
image{
  width: 100%;
  height: 100%;
}
.icon{
  margin-top: 50rpx
}
.icon:nth-child(3){
  margin-top: 0;
}
.icon .image{
  width: 51.48rpx;
  height: 49.14rpx;
  padding-bottom: 11.7rpx;
}
.icon .icon-upload{
  width: 114.66rpx;
  height: 114.66rpx;
}
