.icon .icon-mine{
  width: 37.44rpx;
  height: 49.14rpx;
  position: relative
}
.tabbar .active{
  color: #34C289;
}
.upload{
  width: 100%;
  height: 100%;
  /* background: #FFFFFF; */
  position: fixed;
  left: 0;
  top: 0;
  z-index: 99;
   /* background-color: rgba(255, 255, 255, 0)  */
}
.upload .mh{
  width: 100%;
   /* height: 1336rpx;  */
  position: absolute;
  left: 0;
  top: 0;
} 
.upload .close{
  width: 28.08rpx;
  height: 28.08rpx;
  position: absolute;
  left: 50%;
  bottom: 70.2rpx;
  transform: translateX(-14.04rpx)
}
.upload .upload_icon{
  display: flex;
  width: 100%;
  justify-content: space-around;
  position: absolute;
  left: 0;
  bottom: 238.68rpx;
}
.upload .upload_icon image{
  width: 126.36rpx;
  height: 126.36rpx;
  padding-bottom: 35.1rpx
}
.upload .upload_icon view{
  display: flex;
  flex-direction: column;
  align-items: center;
}
.upload .upload_icon view text{
  font-size: 23.4rpx;
  color: #333
}
